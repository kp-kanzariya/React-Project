{
    "loop": true,
    "speed": 600,
    "autoplay": {
      "delay": 5000
    },
    "slidesPerView": "auto",
    "pagination": {
      "el": ".swiper-pagination",
      "type": "bullets",
      "clickable": true
    },
    "navigation": {
      "nextEl": ".js-custom-next",
      "prevEl": ".js-custom-prev"
    },
    "breakpoints": {
      "320": {
        "slidesPerView": 1,
        "spaceBetween": 40
      },
      "1200": {
        "slidesPerView": 3,
        "spaceBetween": 40
      }
    }
  }